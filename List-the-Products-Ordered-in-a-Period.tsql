select product_name, sum(unit) as unit from(select p.product_id, p.product_name, o.order_date, o.unit from products as p inner join orders as o on p.product_id = o.product_id where datepart(month, order_date)=2 and datepart(year, order_date)=2020) as z group by product_name having sum(unit) >=100